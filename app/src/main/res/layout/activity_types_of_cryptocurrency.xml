                    android:textColor="@color/red"
                    android:textSize="30dp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/avalanche"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginBottom="10dp"
                    android:layout_toRightOf="@+id/t11"
                    android:background="@drawable/language"
                    android:elevation="10dp"
                    android:gravity="left|center"
                    android:onClick="showAvalanche"
                    android:text="@string/avalanche"
                    android:textAllCaps="false"
                    android:textColor="@color/black"
                    android:textSize="15dp"
                    android:textStyle="bold" />


            </RelativeLayout>

            <com.github.aakira.expandablelayout.ExpandableRelativeLayout
                android:id="@+id/ex11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rl11"
                android:paddingLeft="2dp"
                android:paddingRight="2dp"
                app:ael_duration="300"
                app:ael_expanded="false"
                app:ael_interpolator="accelerate"
                app:ael_orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/avalancheSen"
                    android:textColor="@color/white"
                    android:textSize="15dp" />

            </com.github.aakira.expandablelayout.ExpandableRelativeLayout>


        </RelativeLayout>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>

